{
  "name": "gluestack-starter-expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "fix": "expo install --pnpm --fix",
    "migrate": "npx --yes drizzle-kit generate",
    "dev": "expo run:android",
    "dev:web": "expo start -c --web",
    "dev:android": "expo start -c --android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start -c --web",
    "env": "eas secret:push --scope project --env-file .env.local",
    "env:list": "eas secret:list",
    "clean": "rm -rf .expo node_modules",
    "postinstall": "npx tailwindcss -i ./global.css -o ./node_modules/.cache/nativewind/global.css",
    "prebuild": "expo prebuild",
    "preview": "expo prebuild && eas build --profile preview --platform android",
    "build": "expo prebuild && eas build --profile development --platform android",
    "prod": "expo prebuild && eas build --profile production --platform android"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gluestack-ui/nativewind-utils": "^1.0.25",
    "@gluestack-ui/overlay": "^0.1.15",
    "@gluestack-ui/toast": "^1.0.7",
    "@react-navigation/native": "^6.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "expo": "~51.0.28",
    "expo-build-properties": "^0.12.5",
    "expo-constants": "~16.0.2",
    "expo-font": "~12.0.9",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-web-browser": "~13.0.3",
    "jscodeshift": "0.15.2",
    "nativewind": "4.0.36",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "tailwindcss": "^3.4.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4"
}
